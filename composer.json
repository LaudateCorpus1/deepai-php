{
    "name": "r11baka/deepai",
    "description": "Simple deepai.org colorization client",
    "type": "library",
    "keywords": [
        "colorization",
        "deepai.org",
        "blackandwhite"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alex",
            "email": "r11baka@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "R11baka\\Deepai\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "R11baka\\Deepai\\Tests\\": "tests"
        }
    }
}
